{
    "name": "fobia/php-object-collection",
    "type": "library",
    "description": "PHP Object Collection",
    "license": "MIT",
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-4": { "Fobia\\": "src/"},
        "exclude-from-classmap": [ "/tests/" ]
    },
    "autoload-dev": {
        "psr-4": { "Fobia\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
